<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
	<configuration>

		<!-- report theme -->
		<!-- standard, dark -->
		<theme>dark</theme>

		<!-- document encoding -->
		<!-- defaults to UTF-8 -->
		<encoding>UTF-8</encoding>

		<!-- protocol for script and stylesheets -->
		<!-- defaults to https -->
		<protocol>http</protocol>

		<!-- title of the document -->
		<documentTitle>Extent Report</documentTitle>

		<!-- report name - displayed at top-nav -->
		<reportName>Smoke Report</reportName>

		<!-- global date format override -->  <!-- defaults to dd-MM-yyyy -->
		<dateFormat>dd-MM-yyyy</dateFormat>

		<!-- global time format override -->   <!-- defaults to HH:mm:ss -->
		<timeFormat>HH:mm:ss</timeFormat>

		<!-- location of charts in the test view -->
		<!-- top, bottom -->
		<testViewChartLocation>top</testViewChartLocation>
		
		<!-- custom javascript -->
		<!-- offlineMode>true</offlineMode -->
		<scripts>

<![CDATA[
$(document).ready(function() {
});
]]>
		</scripts>
		<!-- custom styles -->
		<styles>
<![CDATA[
]]>
		</styles>
	</configuration>
</extentreports>